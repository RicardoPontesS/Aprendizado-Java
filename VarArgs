package videoaulas;

public class ExemploVarargs {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] vetor;
		soma(10,20,30);
	}

	static void soma(Integer... vetor) {
		int total = 0;

		for (int i = 0; i < vetor.length; i++) {
			total = total + vetor[i];
		}
		
		System.out.println("Total da soma: " + total);
	}
}
